    "%load queues\n",
    "\n",
    "void iterate(Stack stack) {\n",
    "    Stack tmp = new Stack();\n",
    "\n",
    "    // get value from stack\n",
    "    // store it to temporary stack\n",
    "    // use the value\n",
    "    while (!stack.isEmpty()) {\n",
    "        int x = stack.pop();\n",
    "        tmp.push(x);\n",
    "\n",
    "        print(x);\n",
    "    }\n",
    "\n",
    "    // restore the original stack\n",
    "    while (!tmp.isEmpty()) {\n",
    "        stack.push(tmp.pop());\n",
    "    }\n",
    "}"
   ]
