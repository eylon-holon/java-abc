    "%load queues\n",
    "\n",
    "void iterate(Queue que) {\n",
    "    Queue tmp = new Queue();\n",
    "\n",
    "    // get value from queue\n",
    "    // store it to temporary queue\n",
    "    // use the value\n",
    "    while (!que.isEmpty()) {\n",
    "        int x = que.remove();\n",
    "        tmp.insert(x);\n",
    "\n",
    "        print(x);\n",
    "    }\n",
    "\n",
    "    // restore the queue\n",
    "    while (!tmp.isEmpty()) {\n",
    "        que.insert(tmp.remove());\n",
    "    }\n",
    "}"
   ]
